package com.itbank.dto;

public class PersonDTO {
	private String name;
	private int age;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
}
//getter setter 가 있어야 el태그에서 접근가능