package main;

import obj.Toy;

public class Ex01 {
	public static void main(String[] args) {
		Toy t1 = new Toy();
		Toy t2 = t1;
		
		Toy t3  = new Toy();
		
		System.out.println("t1 = " + t1);
		System.out.println("t2 = " + t2);
		System.out.println("t3 = " + t3);
		//해쉬코드가 같다 = 같은 것을 참조
		//해쉬코드가 다르다 = 다른 것을 참조
	}
}
